<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <meta charset="utf-8">
    <title>Adding a File</title>
    
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
    <script src="/javascripts/file.js"></script>
   
  </head>
  <body ng-app="fileModule"   ng-controller="fileController">

    <div >
        <form novalidate>
            Name:<br>
            <input type="text" ng-model="name"><br>
            Phone:<br>
            <input type="phone" ng-model="phone"><br>
            Live:<br>
            <input type="text" ng-model="live"  > <br>
            Paid:<br>
            <input type="number" ng-model="paid"  > <br>
            Type:<br>
            <input type="text" ng-model="type"><br>
            Size:<br>
            <input type="text" ng-model="size"><br>
            Purpose:<br>
            <input type="text" ng-model="purpose"><br>
            Remarks:<br>
            <input type="text" ng-model="remarks"><br>
            Vendor:<br>
            <input type="text" ng-model="vendor"><br>
            <br><br>
       
         
        <input type="file" id="myFileInput" />
        <br /><br />
        <button ng-click="submit()">
          SubmitTrackpoints
        </button>
        <br />
        <br />
        <input type="file" id="myFileWayPointInput" />
        <br />
        <button ng-click="submitWaypoints()">
          submitWaypoints
        </button>
         </form>
        <br /><br />
        <br /><br />
        <h1>
          Filename: {{ fileName }}
        </h1>
        <h2>
          File size: {{ fileSize }} Bytes
        </h2>
        <h2>
          File Content: {{ fileContent }}
        </h2>
      </div>
  </body>
</html>
